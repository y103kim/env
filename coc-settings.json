{
  "http.proxyStrictSSL": false,
  "python.analysis.typeCheckingMode": "off",
  "python.formatting.provider": "yapf",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "ts",
    "tsx",
    "json",
    "js",
    "jsx",
    "css",
    "go",
    "markdown"
  ]
}
